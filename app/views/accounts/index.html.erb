<div class="container">
  <div class="row">
    <div class="col-8">
      <% @posts.each do |post| %>

        <div class="text-white">
        <%= post.account.email %>
        </div>
        
        <%= image_tag post.image.url, class: "img img-fluid"  %>
      <% end %>
    </div>
  </div>
</div>